<?xml version="1.0"?>
<config>
    <global>
        <models>
            <library_rewrite>
                <class>Danslo_LibraryRewrite_Model</class>
            </library_rewrite>
        </models>
        <events>
            <resource_get_tablename>
                <observers>
                    <add_autoloader>
                        <class>Danslo_LibraryRewrite_Model_Observer</class>
                        <method>addAutoloader</method>
                    </add_autoloader>
                </observers>
            </resource_get_tablename>
        </events>
    </global>
</config>
